<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">
    <bean class="com.github.ocroquette.teamcity.autopin.AutoPinBuildServerListener"></bean>
    <bean class="com.github.ocroquette.teamcity.autopin.AddBuildTagMessageTranslator"></bean>
    <bean class="com.github.ocroquette.teamcity.autopin.RequestPinningMessageTranslator"></bean>
    <bean class="com.github.ocroquette.teamcity.autopin.AutoPinBuildFeature"></bean>
    <bean class="com.github.ocroquette.teamcity.autopin.AutoTagBuildFeature"></bean>
</beans>